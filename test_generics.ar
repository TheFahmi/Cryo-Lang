// Test Generic Syntax Parsing
// This file tests that the compiler can parse generic syntax
// without implementing actual generic type checking

// Generic struct with one type parameter
struct Box<T> {
    value: T
}

// Generic struct with multiple type parameters
struct Pair<K, V> {
    key: K,
    val: V
}

// Generic function (syntax only)
fn wrap<T>(x: T) {
    return Box { value: x };
}

// Function with generic parameter types
fn get_value<T>(b: Box<T>) {
    return b.value;
}

fn main() {
    print("=== Generic Syntax Tests ===");
    
    // Create Box with int
    let b1 = Box { value: 42 };
    print("Box value: " + b1.value);
    
    // Create Box with string
    let b2 = Box { value: "hello" };
    print("Box value: " + b2.value);
    
    // Create Pair
    let p = Pair { key: "name", val: 123 };
    print("Pair key: " + p.key);
    print("Pair val: " + p.val);
    
    // Use wrap function
    let b3 = wrap(100);
    print("Wrapped: " + b3.value);
    
    // Use get_value
    let v = get_value(b1);
    print("Got: " + v);
    
    print("=== All Tests Passed ===");
    return 0;
}
