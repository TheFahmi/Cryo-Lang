enum Result {
    Ok(val),
    Err(msg)
}

fn main() {
    print("Testing Enums (v2.5)...");
    
    let res = Ok(42);
    
    match res {
        Ok(n) => {
            print("Success! Value: " + n);
        },
        Err(e) => {
            print("Error: " + e);
        }
    }
    
    let err = Err("Fatal Error");
    
    match err {
        Ok(n) => {
            print("Should not see this");
        },
        Err(e) => {
            print("Caught expected error: " + e);
        }
    }
}
