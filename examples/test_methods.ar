// Method test with arithmetic

struct Point {
    x: int,
    y: int
}

fn Point.get_x(self) {
    return self.x;
}

fn Point.get_y(self) {
    return self.y;
}

fn Point.sum(self) {
    return self.x + self.y;
}

fn main() {
    print("=== Method Test ===");
    
    let p = Point { x: 10, y: 20 };
    
    print("p.get_x() =");
    print(p.get_x());
    
    print("p.get_y() =");
    print(p.get_y());
    
    print("p.sum() =");
    print(p.sum());
    
    print("=== Done ===");
}
