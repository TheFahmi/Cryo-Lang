// Demo Array Indexing

fn main() {
    const arr = [10, 20, 30, 40];
    
    print("Array: ");
    print(arr);
    
    print("arr[0] = " + arr[0]);
    print("arr[2] = " + arr[2]);
    
    const nested = [ [1, 2], [3, 4] ];
    // Nested indexing not yet supported smoothly in one go like arr[0][0] unless primary loop handles it correctly.
    // Let's test single level first.
    
    const users = ["Alice", "Bob"];
    print("User 1: " + users[1]);
}
