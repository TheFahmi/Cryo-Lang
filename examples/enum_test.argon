enum TokenType {
    Integer,
    Plus,
    Minus,
    String,
    Identifier
}

fn main() {
    // Access enum variants
    print("Enum variants:");
    print(TokenType.Integer);
    print(TokenType.Plus);
    print(TokenType.String);
    
    // Store in variable
    let tok = TokenType.Integer;
    print("Current token:");
    print(tok);
    
    // Compare
    if (tok == TokenType.Integer) {
        print("Token is Integer!");
    }
    
    if (tok == TokenType.Plus) {
        print("Token is Plus!");
    } else {
        print("Token is NOT Plus.");
    }
}
